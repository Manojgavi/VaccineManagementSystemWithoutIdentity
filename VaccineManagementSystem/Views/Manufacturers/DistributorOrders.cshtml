@model IEnumerable<VaccineManagementSystem.ViewModel.ManufacturerOrdersViewModel>

@{
    ViewBag.Title = "DistributorOrders";
}
<body style="background-color:lightcyan;">
    <h2>DistributorOrders</h2>


    <table class="table">
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.DistributorName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Count)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.DistributorName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Count)
                </td>
                <td>
                    @Html.ActionLink("Delivered", "Delivered", new { id = item.Id })
                    @*@Html.ActionLink("Details", "Details", new { id=item.Id }) |
                        @Html.ActionLink("Delete", "Delete", new { id=item.Id })*@
                </td>
            </tr>
        }

    </table>
</body>